<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.achuan.studentmanagersystem">

    <application
        android:allowBackup="true"
        android:icon="@drawable/school_badge"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!--activity注册-->
        <activity android:name=".acitivity.ShowStudentInfo"></activity>
        <activity android:name=".acitivity.EditorStudentActivity"></activity>
        <activity android:name=".acitivity.ManagerActivtiy"></activity>
        <activity android:name=".acitivity.registerActivity"></activity>
        <activity android:name=".acitivity.loginActivity"></activity>
        <activity android:name=".acitivity.FirstOpenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>